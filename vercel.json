{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "env": {
    "VITE_SUPABASE_URL": {
      "description": "Supabase project URL",
      "value": ""
    },
    "VITE_SUPABASE_ANON_KEY": {
      "description": "Supabase anonymous public key",
      "value": ""
    }
  },
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
